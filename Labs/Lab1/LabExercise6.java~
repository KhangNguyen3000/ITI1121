			grades[i] = sc.nextInt();
		}
	}
	
	public static void calculateAverage(double[] notes){
				double result;
		result = 0;
		for(int i = 0; i < notes.length; i++){
			result = result + notes[i];
			
		}
		result = (result/notes.length);
		return result;
	}
	
